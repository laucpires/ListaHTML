.corpo {
    border: none;
    width: 100%;
    font-family: Arial;
    font-size: 12px;
}

caption {
    background-color: #0D5188;
    color: #fff;
    font-weight: bold;
    width: 549px;
}

.cabecalho {
    background-color: #66C5E3;
    font-weight: bold;
}

